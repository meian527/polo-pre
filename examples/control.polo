fn main() -> i32 {
    let x: i32 = 10;
    let y: i32 = 5;
    let result: i32 = 0;
    
    if x > y {
        result = x - y;
    } else {
        result = y - x;
    }
    
    let i: i32 = 0;
    for i < 5 {
        result = result + i;
        i = i + 1;
        syscall!(1, 1, "Hello\n", 6);
    }
    
    return result;
}
